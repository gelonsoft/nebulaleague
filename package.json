{
    "name": "Nebulaleague",
    "version": "1.0.2",
    "description": "Nebulaleague",
    "engines": {
        "node": "16.5.0"
    },
    "scripts": {
        "schema": "set NODE_ENV=development && npx env-cmd -f ./.env.development npx ts-node src/generateSample.ts",
        "clean": "npx rimraf public/*.js public/css public/*.txt public/index.html",
        "dev": "set NODE_ENV=development && tsc-watch --onSuccess \"npx env-cmd -f ./.env.development node dist/server/server.js\"",
        "dev:d": "set NODE_ENV=development && tsc-watch --onSuccess \"node --inspect dist/server/server.js\"",
        "dev:c": "set NODE_ENV=development &&  node inspect dist/server/server.js",
        "dev:client.old": "npx webpack serve --config client/webpack/dev.config.js",
        "dev:client": "npx webpack serve --config webpack.dev.config.js",
        "build:client": "webpack --config webpack.prod.config.js",
        "build:server": "tsc",
        "prod": "set NODE_ENV=production && npm-run-all client:prod server:prod",
        "predeploy": "npm run clean && npm install",
        "deploy": "set NODE_ENV=production  && npm-run-all build:client build:server",
        "postdeploy": "node dist/server/server.js",
        "start": "npm run dev",
        "lint": "eslint src/**/**/*",
        "lint:server": "eslint src/server/**/*",
        "lint:shared": "eslint src/shared/**/*",
        "lint:client": "eslint src/client/**/*"
    },
    "author": "edil3ra",
    "license": "MIT",
    "_moduleAliases": {
        "~": "dist/."
    },
    "dependencies": {
        "@colyseus/core": "^0.14.19",
